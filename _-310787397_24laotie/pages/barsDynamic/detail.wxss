.page {
    background: #f5f5f5;
}

.header {
    border-bottom: 1px solid #eee;
    height: 130rpx;
    background: #fff;
    padding: 5px 10px 0px 10px;
}

.head {
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
    overflow: hidden;
    border: 1px solid #f4de3b;
    float: left;
    margin: 5rpx 10rpx;
}

.head image {
    width: 100%;
    height: 100%;
}

.username {
    float: left;
    line-height: 110rpx;
    font-size: .75rem;
}

.icon-add {
    width: 30rpx;
    height: 30rpx;
    margin-right: 8rpx;
    vertical-align: baseline;
}

.add {
    float: right;
    margin-right: 40rpx;
}

.add button {
    background: #8bc34a;
    color: #fff;
    width: 130rpx;
    font-size: .82rem;
    height: 50rpx;
    padding: 10rpx;
    line-height: 30rpx;
    border-radius: 10rpx;
    margin-top: 30rpx;
}

.content {
    clear: both;
    margin-top: 20rpx;
    background: #fff;
}

.text {
    font-size: 30rpx;
    padding: 10rpx;
    margin-top: 20rpx;
    background: #fff;
}

.p {
    position: relative;
}

.icon {
    width: 50rpx;
    height: 50rpx;
    margin: 10rpx;
    position: absolute;
}

.p text {
    margin-left: 60rpx;
    font-size: .9rem;
}

.icon-tel {
    width: 60rpx;
    height: 60rpx;
    margin-left: 50rpx;
}

.contact text {
    font-size: .94rem;
    line-height: 80rpx;
    margin: 20rpx;
}

.tit {
    color: #000;
}

.icon-departure {
    bottom: -6rpx;
}

.icon-gender {
    width: 30rpx;
    height: 30rpx;
    top: 16rpx;
}

.tocomment {
    width: 100rpx;
    float: left;
}

button::after {
    display: none;
}

.item_bottom_right image {
    width: 20rpx;
    height: 20rpx;
    margin-left: 21rpx;
}

.item_bottom_right {
    float: right;
    font-size: 28rpx;
    line-height: 48rpx;
    margin-top: 16rpx;
    border-radius: 6rpx;
    margin-right: 42rpx;
    width: 130rpx;
    height: 50rpx;
    text-align: center;
    background: #1296DB;
    color: #fff;
    border: none;
}

.report {
    font-size: 30rpx;
    text-align: right;
    color: #000;
    margin-top: 5rpx;
}

.page_tips {
    background: #f9dd90;
    padding: 0 10rpx 10rpx;
}

.page_tips text {
    color: #e13c40;
    font-size: 24rpx;
    line-height: 30rpx;
}

.imgtext {
    background: #fff;
    margin-top: 20rpx;
}

.title {
    text-align: center;
    font-weight: bold;
    color: #333;
    padding: 10rpx;
}

.imglist {
    overflow: hidden;
}

.imglist .item {
    width: 320rpx;
    height: 240rpx;
    margin-left: 37rpx;
    float: left;
}

.imglist .item image {
    width: 320rpx;
    height: 240rpx;
}