.page image {
    width: 750rpx;
}

.imglist {
    width: 750rpx;
    height: 350rpx;
    overflow: hidden;
}

.imglist image {
    width: 750rpx;
    display: block;
}

.imglist text {
    position: absolute;
    right: 20rpx;
    top: 280rpx;
    color: #fff;
    background: rgba(0,0,0,0.4);
    padding: 10rpx 15rpx;
    font-size: 24rpx;
}

.prodetail {
    background: #fff;
    margin-bottom: 20rpx;
}

.prodetail .title {
    color: #333;
    padding: 10rpx 20rpx;
    font-size: 32rpx;
    line-height: 40rpx;
}

.prodetail .text {
    padding: 10rpx;
    font-size: 24rpx;
    line-height: 48rpx;
    text-indent: 48rpx;
}

.prodetail .price {
    color: #ff811a;
    font-size: 48rpx;
    text-indent: 20rpx;
}

.prodetail .price text {
    font-size: 30rpx;
}

.prodetail .tips {
    font-size: 24rpx;
    color: #666;
    padding: 10rpx 20rpx;
    border-top: 1rpx solid #eee;
}

.infoDialog {
    position: fixed;
    width: 710rpx;
    margin: 20rpx;
    top: 50%;
    transform: translateY(-50%);
}

.infoDialog .close {
    position: absolute;
    top: -30rpx;
    right: 0;
}

.infoDialog .close image {
    width: 50rpx;
    height: 50rpx;
}

.videolist video {
    width: 750rpx;
}

.navbottom {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 750rpx;
    background: #fff;
    border-top: 1rpx solid #eee;
}

.phone {
    width: 200rpx;
    float: left;
    font-size: 32rpx;
    text-align: center;
    border-right: 1rpx solid #eee;
}

.phone image {
    width: 25rpx;
    height: 30rpx;
}

.concatservice {
    position: relative;
    width: 200rpx;
    float: left;
    font-size: 32rpx;
    text-align: center;
}

.concatservice button {
    opacity: 0;
    position: absolute;
    left: 0;
    top: 0;
    width: 100rpx;
    height: 100rpx;
}

.concatservice image {
    width: 35rpx;
    height: 30rpx;
}

.onceplace {
    float: right;
}

.onceplace button {
    color: #fff;
    background: #ff811a;
    width: 350rpx;
    height: 100rpx;
    line-height: 100rpx;
    font-size: 32rpx;
    border-radius: 0;
}

.starttime {
    background: #fff;
    padding: 10rpx 20rpx;
}

.starttime .title {
    font-size: 28rpx;
    color: #333;
}

.starttime .radio {
    margin-right: 10rpx;
}

.starttime .radio text {
    color: #666;
    font-size: 24rpx;
}

.reback,.cost,.reserve,.intro {
    background: #fff;
    padding: 10rpx 20rpx 80rpx 20rpx;
    font-size: 30rpx;
    margin-top: 20rpx;
}

.reback .title,.cost .title,.reserve .title,.intro .title {
    color: #333;
    font-weight: bold;
}

.reback .content,.cost .content,.reserve .content,.intro .content {
    font-size: 26rpx;
    line-height: 48rpx;
    color: #333;
}