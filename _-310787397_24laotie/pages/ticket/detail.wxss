.page image {
    width: 750rpx;
}

.title {
    font-weight: bold;
    padding: 10rpx;
    font-size: 32rpx;
    line-height: 64rpx;
}

.text {
    padding: 10rpx;
    font-size: 24rpx;
    line-height: 48rpx;
    text-indent: 48rpx;
}

.imglist image {
    width: 750rpx;
}

.price {
    color: red;
    font-weight: bold;
}

.concatservice {
    position: fixed;
    right: 10rpx;
    top: 50vh;
    width: 100rpx;
    height: 100rpx;
}

.concatservice button {
    opacity: 0;
    position: absolute;
    left: 0;
    top: 0;
    width: 100rpx;
    height: 100rpx;
}

.concatservice image {
    width: 100rpx;
    height: 100rpx;
}

.title button {
    border-radius: 5rpx;
    color: #fff;
    background: #f00;
    float: right;
    width: 200rpx;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 32rpx;
}

.infoDialog {
    position: fixed;
    width: 710rpx;
    margin: 20rpx;
    top: 50%;
    transform: translateY(-50%);
}

.infoDialog .close {
    position: absolute;
    top: -30rpx;
    right: 0;
}

.infoDialog .close image {
    width: 50rpx;
    height: 50rpx;
}

.videolist video {
    width: 750rpx;
}